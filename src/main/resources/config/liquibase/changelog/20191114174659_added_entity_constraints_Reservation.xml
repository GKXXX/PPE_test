<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Reservation.
    -->
    <changeSet id="20191114174659-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="locaux_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation_locaux_id"
                                 referencedColumnNames="id"
                                 referencedTableName="locaux"/>

        <addForeignKeyConstraint baseColumnNames="equipe_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation_equipe_id"
                                 referencedColumnNames="id"
                                 referencedTableName="equipe"/>

        <addForeignKeyConstraint baseColumnNames="coach_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation_coach_id"
                                 referencedColumnNames="id"
                                 referencedTableName="coach"/>

    </changeSet>
</databaseChangeLog>
